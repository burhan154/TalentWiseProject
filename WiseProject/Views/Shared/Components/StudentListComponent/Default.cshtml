@using Microsoft.AspNetCore.Identity
@inject SignInManager<ApplicationUser> SignInManager
@model WiseProject.Models.Dto.EnrolledStudents

<div class="col-lg-4">
    <div class="row">
        <div class="col-lg-12 col-md-6">
            <div class="course-features mt-30">
                <h4>Course Features </h4>
                <ul>
                    
                    <li><i class="fa fa-clone"></i>Assignments : <span>@Model.CourseCount</span></li>

                    <li><i class="fa fa-user-o"></i>Students :  <span>@Model.EnrollmentCount</span></li>
                    @foreach (var name in Model.Names)
                    {
                        <li><i class="fa fa-user-o"></i>@name</li>
                    }
                </ul>
                
            </div> <!-- course features -->
        </div>
    </div>
</div>
